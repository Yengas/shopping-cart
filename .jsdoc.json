{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./src/shopping-cart", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|coverage)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "docdash": {
    "static": true,
    "sort": false
  }
}
