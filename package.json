{
  "name": "shopping-cart",
  "version": "1.0.0",
  "description": "Shopping cart OOP practice, with CLI",
  "main": "index.js",
  "scripts": {
    "start": "node bin/index",
    "test": "jest",
    "test-watch": "npm run test -- --watch",
    "lint": "eslint --ignore-pattern \"node_modules/**/*.js\" \"**/*.js\"",
    "lint-fix": "npm run lint -- --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,jsx,ts,tsx}": [
      "npm test -- --bail --findRelatedTests",
      "npm run lint-fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Yengas/shopping-cart.git"
  },
  "keywords": [
    "shopping",
    "cart",
    "nodejs",
    "oop",
    "jest"
  ],
  "author": "Yengas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yengas/shopping-cart/issues"
  },
  "homepage": "https://github.com/Yengas/shopping-cart#readme",
  "devDependencies": {
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "lint-staged": "^8.1.5"
  },
  "dependencies": {
    "clear": "^0.1.0",
    "cli-table": "^0.3.1",
    "readline-sync": "^1.4.9"
  }
}
